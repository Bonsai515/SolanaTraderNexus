# Deployment Environment Variables

# Primary Solana RPC endpoint - this endpoint will be tried first
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Additional RPC endpoints can be added in the RPC manager

# Fallback strategy
USE_PRICE_AGGREGATOR=true
USE_LOCAL_ENDPOINTS=true

# System configuration
MEMORY_LIMIT_MB=8192
CACHE_EXPIRY_MS=60000
RATE_LIMIT_WINDOW_MS=60000

# Enable price caching
ENABLE_PRICE_CACHE=true